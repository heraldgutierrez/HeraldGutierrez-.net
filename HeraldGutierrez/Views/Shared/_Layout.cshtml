<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Herald Gutierrez | @ViewBag.Title</title>
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Assets/Stylesheets/styles.css")" />
    @Styles.Render("~/Assets/Bundles/Scripts")
    @*@Scripts.Render("~/bundles/modernizr")*@

</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="col-sm-12 navbar-header center">
                <div class="row">
                    <h1>Herald Gutierrez</h1>
                    <div class="col-sm-12">
                        <a href="mailto:heraldgutierrez@gmail.com">HeraldGutierrez@gmail.com</a>
                    </div>
                </div>
                <div class="row">
                    <ul class="col-sm-12 nav navbar-nav">
                        <li class="col-sm-12 col-md-3">@Html.ActionLink("Resume", "Resume")</li>
                        <li class="col-sm-12 col-md-3"><a href="https://github.com/heraldgutierrez" target="_blank">GitHub</a></li>
                        <li class="col-sm-12 col-md-3"><a href="https://www.linkedin.com/in/herald-gutierrez" target="_blank">Linked In</a></li>
                        <li class="col-sm-12 col-md-3">@Html.ActionLink("Daily Programmer", "DailyPragrammer", "DailyProgrammer")</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="content">

        <div class="row">
            <div class="col-sm-12">
        @RenderBody()

            </div>
        </div>
    </div>
    
    @RenderSection("scripts", required: false)
</body>
</html>
