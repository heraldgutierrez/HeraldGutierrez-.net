<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Herald Gutierrez | @ViewBag.Title</title>
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300" rel="stylesheet" type="text/css">
    <link type="text/css" rel="stylesheet" href="@Url.Content("~/Assets/Stylesheets/styles.css")" />
    @Styles.Render("~/Assets/Bundles/Scripts")

</head>
<body>
    <div class="container">
        <div class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="col-sm-12 navbar-header center">
                    <div class="row">
                        <h1>Herald Gutierrez</h1>
                        <div class="col-sm-12">
                            <a href="mailto:heraldgutierrez@gmail.com">HeraldGutierrez@gmail.com</a>
                        </div>
                    </div>
                    <div class="row">
                        <ul class="col-sm-12 nav navbar-nav">
                            <li class="col-xs-12 col-sm-4">@Html.ActionLink("Resume", "Resume", "Default")</li>
                            <li class="col-xs-12 col-sm-4"><a href="https://github.com/heraldgutierrez" target="_blank">GitHub</a></li>
                            <li class="col-xs-12 col-sm-4"><a href="https://www.linkedin.com/in/herald-gutierrez" target="_blank">Linked In</a></li>
                            @*<li class="col-xs-12 col-sm-3">@Html.ActionLink("Daily Programmer", "Index", "DailyProgrammer")</li>*@
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container content">
        @RenderBody()
    </div>
    
    @RenderSection("scripts", required: false)
</body>
</html>
